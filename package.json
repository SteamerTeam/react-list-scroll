{
  "name": "react-list-scroll",
  "version": "1.0.1",
  "description": "react scroll for list",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node ./tools/start.js",
    "start.code": "node ./tools/start.code.js",
    "dev": "npm start",
    "dev.code": "npm run start.code",
    "dist": "babel src --out-dir dist --copy-files",
    "prepublish": "npm run dist",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js && stylelint src/**/*.less"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteamerTeam/react-list-scroll.git"
  },
  "keywords": [
    "react",
    "pure render",
    "deep compare"
  ],
  "author": "heyli",
  "license": "ISC",
  "dependencies": {
    "global": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-res-webpack-plugin": "^1.3.3",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^19.0.2",
    "jest-environment-jsdom": "^19.0.2",
    "jsdom": "^9.11.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "preact": "^7.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "steamer-webpack-utils": "^0.1.2",
    "style-loader": "^0.13.2",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
